{
    "name": "node-express-webpack-typescript-starter",
    "version": "0.0.1",
    "description": "Node express with Typescript and webpack starter",
    "main": "index.js",
    "author": "Aditya Lucky Septiadi",
    "license": "MIT",
    "engines": {
        "node": ">= 10"
    },
    "scripts": {
        "clean": "yarn clean:dist",
        "clean:dist": "rm -rf dist/",
        "build:dev": "yarn clean:dist && webpack --mode=development",
        "build:prod": "yarn clean:dist && webpack --mode=production",
        "dev:serve": "yarn build:dev && NODE_ENV=development node dist/index.js",
        "prod:serve": "yarn build:prod && NODE_ENV=production node dist/index.js"
    },
    "dependencies": {
        "@types/cors": "^2.8.7",
        "@types/express": "^4.17.7",
        "@types/node": "^14.6.0",
        "@types/node-fetch": "2.5.7",
        "body-parser": "^1.19.0",
        "chalk": "4.1.0",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "node-fetch": "2.6.0"
    },
    "devDependencies": {
        "@babel/cli": "7.2.3",
        "@babel/core": "7.6.3",
        "@babel/plugin-proposal-nullish-coalescing-operator": "7.8.3",
        "@babel/plugin-proposal-numeric-separator": "7.8.3",
        "@babel/plugin-proposal-object-rest-spread": "7.9.0",
        "@babel/plugin-proposal-optional-chaining": "7.9.0",
        "@babel/plugin-transform-destructuring": "7.8.8",
        "@babel/plugin-transform-runtime": "7.9.0",
        "@babel/preset-env": "7.9.0",
        "@babel/preset-typescript": "7.9.0",
        "@types/react-router-dom": "^5.1.5",
        "@typescript-eslint/eslint-plugin": "^3.4.0",
        "@typescript-eslint/parser": "^3.4.0",
        "babel-loader": "8.0.5",
        "eslint": "^7.3.1",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "prettier": "^2.0.5",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-router-dom": "^5.2.0",
        "ts-loader": "8.0.2",
        "typescript": "^3.9.5",
        "webpack": "4.29.6",
        "webpack-cli": "^3.3.12",
        "webpack-node-externals": "^2.5.1"
    },
    "keywords": [
        "express",
        "node",
        "javascript",
        "webpack",
        "starter"
    ]
}
